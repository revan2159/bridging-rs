{
    "name": "rskbdev/bridging-bpjs",
    "description": "Bridging BPJS Yang Bikin PUSING",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.1",
        "nullpunkt/lz-string-php": "^1.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bpjs\\Bridging\\": "src/Bpjs/",
            "Kemkes\\Bridging\\": "src/kemkes"
        }
    },

    "config": {
        "sort-packages": true
      },
    "extra": {
        "laravel": {
          "providers": [
            "Bpjs\\Bridging\\BridgingBpjsServiceProvider"
          ]
        }
    },

    "authors": [
        {
            "name": "rskbdev",
            "email": "rskbdev@mail.com"
        }
    ]
}
